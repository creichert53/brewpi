{
  "name": "material-ui-app",
  "version": "0.1.0",
  "description": "App created with create-react-app and utilizing an express backend with material-ui frontend.",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "start-production": "npm run build && REACT_APP_SERVER_PORT=80 NODE_ENV=production nodemon server/server.js",
    "start-dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/server.js",
    "client": "REACT_APP_SERVER_PORT=3001 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-regenerator": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "bake-cli": "^0.1.6",
    "eslint": "^2.10.2",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "mocha": "^2.5.2",
    "standard-version": "^2.2.1",
    "watchd": "github:mklabs/watchd",
    "concurrently": "^3.5.1"
  },
  "bake": {
    "description": "Scaffold a basic ES6 setup",
    "scripts": {
      "start": "echo Starting generation of default template",
      "prestart": "echo prestart",
      "poststart": "echo poststart",
      "install": "npm install --loglevel warn --cache-min Infinity",
      "preinstall": "echo Installing dependencies ...",
      "postinstall": "npm ls --depth 0"
    }
  },
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.2.0",
    "@material-ui/icons": "^1.1.0",
    "ajv": "^6.3.0",
    "axios": "^0.18.0",
    "bacstack": "0.0.1-beta.13",
    "bindings": "^1.3.0",
    "bluebird": "^3.5.1",
    "color": "^3.0.0",
    "convert-units": "^2.3.4",
    "cors": "^2.8.4",
    "cron": "^1.3.0",
    "events": "^2.0.0",
    "express": "^4.16.3",
    "gradient-color": "^2.0.1",
    "helmet": "^3.12.0",
    "http-proxy-middleware": "^0.18.0",
    "mathjs": "^4.1.1",
    "mcp-spi-adc": "^2.0.2",
    "nan": "^2.10.0",
    "numeral": "^2.0.6",
    "onoff": "^3.1.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^1.1.4",
    "redux": "^3.7.2",
    "redux-socket.io": "^1.4.0",
    "redux-thunk": "^2.2.0",
    "rethinkdb": "^2.3.3",
    "socket.io": "^2.1.0",
    "socket.io-client": "^2.1.0",
    "traverse": "^0.6.6",
    "url": "^0.11.0",
    "uuid": "^3.2.1",
    "xml2js": "^0.4.19",
    "zero-fill": "^2.2.3"
  },
  "nodemonConfig": {
    "ignore": [
      "server/database/*"
    ]
  }
}
