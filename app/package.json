{
  "name": "material-ui-app",
  "description": "App created with create-react-app and utilizing an express backend with material-ui frontend.",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run build && REACT_APP_SERVER_PORT=80 NODE_ENV=production node server/server.js",
    "start-dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/server.js",
    "client": "REACT_APP_SERVER_PORT=3001 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "ajv": "^6.3.0",
    "axios": "^0.18.0",
    "bacstack": "0.0.1-beta.13",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "helmet": "^3.12.0",
    "http-proxy-middleware": "^0.18.0",
    "material-ui": "^1.0.0-beta.40",
    "material-ui-icons": "^1.0.0-beta.36",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "redux": "^3.7.2",
    "redux-socket.io": "^1.4.0",
    "redux-thunk": "^2.2.0",
    "rethinkdb": "^2.3.3",
    "socket.io": "^2.1.0",
    "socket.io-client": "^2.1.0",
    "url": "^0.11.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  }
}
